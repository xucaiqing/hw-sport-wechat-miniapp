.calendar-box{
  background:#fff;
  border-radius:40rpx;
  box-shadow: 0 0 5px rgba(0, 0, 0, 0.1);
  padding:20rpx;
  margin:20rpx;
}
  
.calendar {
  --calendar-height: 500px;
}
.calendar .van-calendar__header-title{
  height:0;
}
.calendar .van-calendar__header-subtitle{
  background:#f0f0f0;
  border-radius: 20rpx;
  color:#525866;
}
.calendar .van-calendar__selected-day{
  background:#33cb87;
  border-radius: 50%;
  width:90rpx;
  height:90rpx;
}
/* 打卡日历 */
  .all .bar{
  background:#f0f0f0;
  border-radius: 20rpx;
  color:#525866;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  margin: 10rpx 20rpx;
  padding:10rpx 20rpx;
  }
  .all .week{
  color:#666;
  display: flex;
  font-size:30rpx;
  flex-direction: row;
  justify-content: space-between;
  padding: 20rpx;
  padding-left: 40rpx;
  padding-right: 40rpx;
  }
   
  /* .all .days{
  margin: 20rpx;
  padding: 10rpx;
  border-radius: 10px;
  } */
   
  .all .columns{
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  }
   
  .all .columns .rows{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  }
   
  .all .columns .rows .cell{
  color:#525866;
  width: 70rpx;
  height: 70rpx;
  margin: 10rpx;
  text-align: center;
  border-radius: 50%;
  display: flex;
  line-height: 70rpx;
  flex-direction: column;
  justify-content: center;
  }
  .csigned{
    background:#33cb87;
    color:#fff;
    border-radius:50%;
    width: 70rpx;
    height: 70rpx;
    line-height: 70rpx;
  }

  .extraSigned{
    background:#ffb520;
    color:#fff;
    border-radius:50%;
    width: 70rpx;
    height: 70rpx;
    line-height: 70rpx;
  }
   
  .count .daynumber{
  display: flex;
  flex-direction: row;
  justify-content: center;
  }
   
  .count .daynumber .day{
  margin-top: 50rpx;
  }
   
  .count{
  margin: 20rpx;
  padding: 30rpx;
  display: flex;
  text-align: center;
  border-radius: 10px;
  flex-direction: column;
  justify-content: center;
  background-color: #f0f0f0;
  align-items: center;
  }
   
  .count .number{
  color: red;
  font-size: 60rpx;
  background-color: #fff;
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  margin: 20rpx;
  }
   
  .count text{
  margin: 10rpx;
  }
.tag{
  color:#525866;
  font-size:30rpx;
  padding:10rpx 40rpx;
  display:flex;
}
.tag-green{
  background:#33cb87;
  width:40rpx;
  height:40rpx;
  margin-right:10rpx;
}
.tag-yellow{
  background:#ffb520;
  width:40rpx;
  height:40rpx;
  margin-right:10rpx;
}
.tag-gray{
  background:#dedede;
  width:40rpx;
  height:40rpx;
  margin-right:10rpx;
}
.tag-item{
  width:30%;
  display:flex;
}
  .note{
    color:#999;
    font-size:26rpx;
    padding:20rpx 40rpx;
  }
  .note view{
    padding-bottom:14rpx;
  }